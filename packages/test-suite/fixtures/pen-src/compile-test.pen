nested = (
    REP = (a) => a x a
    FUN = x => x x
    x = "inner x"
    a = 42
)

letexpr = (
    lx "-" lx 

    where
    lx = "inner x"
    ly = "***"
)

start = nested.REP(a = x)
      | letexpr



