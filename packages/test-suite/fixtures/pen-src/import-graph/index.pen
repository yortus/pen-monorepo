(f as foo, b as bar, baz) = import './a'
() = import './b.pen'

start = result

digit = `\x30-39`               // 0-9
alpha = `\x61-7a` | `\x41-5a`   // a-ZA-Z

result = foo (bar baz)

myList = [digit, digit digit, digit digit digit]

rec = (
    b = abstract `b thing`
    d = abstract `d thing`
)

r2 = rec
r2d = rec.d
